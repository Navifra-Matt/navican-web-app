# Development Environment Configuration

# CAN Interface Configuration
CAN_INTERFACE=vcan0
CAN_FILTERS=
STATS_INTERVAL=5

# ClickHouse Configuration (Docker defaults)
CLICKHOUSE_HOST=hyperdx-all-in-one
CLICKHOUSE_PORT=9000
CLICKHOUSE_DATABASE=default
CLICKHOUSE_USERNAME=hyperdx_dev_password
CLICKHOUSE_PASSWORD=
CLICKHOUSE_TABLE=can_messages
CLICKHOUSE_STATS_TABLE=can_interface_stats

# InfluxDB Configuration (Docker defaults)
INFLUXDB_URL=http://influxdb3-core:8181
INFLUXDB_TOKEN=my-dev-token
INFLUXDB_ORG=dev-org
INFLUXDB_BUCKET=can_messages

# General Configuration
BATCH_SIZE=100
API_PORT=8080
